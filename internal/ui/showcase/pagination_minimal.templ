package showcase

import "github.com/axzilla/templui/components"

templ PaginationMinimal() {
	@components.Pagination(components.PaginationProps{
		CurrentPage:  4,
		TotalPages:   10,
		Variant:      components.PaginationVariantMinimal,
		MaxVisible:   5,
		ShowNumbers:  true,
		ShowControls: true,
		// UrlPattern:   "/blog?page={page}",
	})
}
