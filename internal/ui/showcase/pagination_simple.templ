package showcase

import "github.com/axzilla/templui/components"

templ PaginationSimple() {
	@components.Pagination(components.PaginationProps{
		CurrentPage:  2,
		TotalPages:   5,
		Variant:      components.PaginationVariantSimple,
		ShowNumbers:  false,
		ShowControls: true,
		// UrlPattern:   "/products?page={page}",
	})
}
